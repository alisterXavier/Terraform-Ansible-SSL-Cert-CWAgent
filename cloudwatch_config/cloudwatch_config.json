{
  "agent": {
    "run_as_user": "root"
  },
  "logs": {
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "/var/log/auth.log",
            "log_group_class": "STANDARD",
            "log_group_name": "SSH-Failed-Auth",
            "log_stream_name": "Failed Auth",
            "retention_in_days": 30
          }
        ]
      }
    }
  }
}
